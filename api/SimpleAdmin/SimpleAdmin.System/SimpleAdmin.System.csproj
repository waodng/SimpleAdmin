		<Compile Remove="Services\Gen\**" />
		<Compile Remove="Services\ImportExport\**" />
		<EmbeddedResource Remove="SeedData\**" />
		<EmbeddedResource Remove="Services\BatchEdit\**" />
		<EmbeddedResource Remove="Services\Gen\**" />
		<EmbeddedResource Remove="Services\ImportExport\**" />
