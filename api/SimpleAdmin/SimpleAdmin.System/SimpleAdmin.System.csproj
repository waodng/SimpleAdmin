		<NoWarn>1701;1702;8616;1591;8618;8619;8629;8602;8603;8604;8625;8765</NoWarn>
		<DocumentationFile>SimpleAdmin.System.xml</DocumentationFile>
		<ImplicitUsings>enable</ImplicitUsings>
